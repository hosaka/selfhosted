MEDIA_DOWNLOADS_TORRENT_PATH=/mnt/pool/storage/media/downloads/torrent
MEDIA_DOWNLOADS_USENET_PATH=/mnt/pool/storage/media/downloads/usenet
MEDIA_RCLONE_SERVE_PATH=/mnt/pool/storage/games

MEDIA_JELLYFIN_CARD=/dev/dri/card1
MEDIA_JELLYFIN_RENDER=/dev/dri/renderD128
MEDIA_JELLYFIN_VIDEO_GROUP=27

MEDIA_TORRENT_VPN_ENABLED=true
MEDIA_TORRENT_VPN_IP_CHECK_DELAY=10
MEDIA_TORRENT_VPN_IP_CHECK_EXIT=false
MEDIA_TORRENT_VPN_LAN_NETWORK=10.10.0.0/24

