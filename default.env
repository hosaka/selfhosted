AUTHELIA_STORAGE_ENCRYPTION_KEY=

# Media
MEDIA_PUID=1000
MEDIA_PGID=1000
MEDIA_UMASK=002

MEDIA_ROOT_PATH=/mnt/pool/storage/media
MEDIA_COLLECTION_PATH=/mnt/pool/storage/media/collection
