# shellcheck disable=all

# General
COMPOSE_PROJECT_NAME=box
TIMEZONE=UTC

# Reverse Proxy
PROXY_PUBLIC_DOMAIN=domain.tld
PROXY_PRIVATE_DOMAIN=box.domain.tld
PROXY_CF_API_EMAIL=
PROXY_CF_API_TOKEN=
PROXY_CF_TUNNEL_TOKEN=

# LLDAP
LLDAP_BASE_DN=dc=domain,dc=tld
LLDAP_USER_PASS=
LLDAP_JWT_SECRET=
LLDAP_KEY_SEED=

# Authelia
AUTHELIA_DB_DATABASE_NAME=authelia
AUTHELIA_DB_USERNAME=authelia
AUTHELIA_DB_PASSWORD=
AUTHELIA_JWT_SECRET=
AUTHELIA_LDAP_PASSWORD=
AUTHELIA_SESSION_SECRET=
AUTHELIA_SMTP_ADDRESS=
AUTHELIA_SMTP_PASSWORD=
